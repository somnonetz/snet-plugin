	<xs:complexType name="stmLaboratoryValues">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="FastingBloodSugarLevel" type="xs:float"/>
					<xs:element name="GlycatedHemoglobin" type="xs:float"/> <!-- de:HbA1c -->
					<xs:element name="TotalCholesterolLevel" type="xs:float"/>
					<xs:element name="LDLCholesterol" type="xs:float"/>
					<xs:element name="HOLCholesterol" type="xs:float"/>
					<xs:element name="LH" type="xs:float"/>
					<xs:element name="Testosterone" type="xs:float"/>
					<xs:element name="Endothelin" type="xs:float"/>
					<xs:element name="Norepinephrine" type="xs:float"/> <!-- de:Noradrenalin -->
					<xs:element name="Nitrotyrosine" type="xs:float"/>
					<xs:element name="Isoprostane" type="xs:float"/>
					<xs:element name="Malondialdehyde" type="xs:float"/> <!-- de:MDA -->
					<xs:element name="ThiobarbituricAcidReactiveSubstances" type="xs:float"/> <!-- de:TBARS -->
					<xs:element name="HsCRP" type="xs:float"/> <!-- en:High-Sensitivity C-reactive protein -->
					<xs:element name="Interleukin6" type="xs:float"/>
					<xs:element name="TumorNecrosisFactorAlpha" type="xs:float"/> <!-- de:TNFa -->
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="stmQuestionnaireValues">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="ESSScore">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="24"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="IIEF5Score">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="25"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="WHO5Score">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="25"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="MDIScore">
						<xs:simpleType>
							<xs:restriction base="xs:integer">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="50"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
