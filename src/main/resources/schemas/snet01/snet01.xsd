	<xs:complexType name="stmInclusionEdStudy">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="IsMale" type="xs:boolean"/>
					<xs:element name="AgeBetween18And80" type="xs:boolean"/>
					<xs:element name="ApneaHypopneaIndex_30" type="xs:boolean"/>
					<xs:element name="StableSexualRelationship" type="xs:boolean"/>
					<xs:element name="InformedConsentDocument" type="xs:boolean"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="stmExclusionEdStudy">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
