<!-- study modules --> 

	<xs:complexType name="stmAnthropometricData">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="Accompanying_Disease" type="snet01:comorbidity" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="stmPhysicalFindings">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="NeckCircumference" type="xs:float"/>
					<xs:element name="SystolicPressure" type="xs:float"/>
					<xs:element name="DiastolicPressure" type="xs:float"/>
					<xs:element name="FEV1" type="xs:float"/>
					<xs:element name="OxygenSaturation" type="xs:float"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	
	<xs:complexType name="stmPsgSleep">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="TimeInBed" type="xs:integer"/>
					<xs:element name="SleepPeriodTime" type="xs:integer"/>
					<xs:element name="TotalSleepTime" type="xs:integer"/>
					<xs:element name="TotalWakeTime" type="xs:integer"/>
					<xs:element name="SleepEfficiency" type="xs:float"/>
					<xs:element name="SleepLatency" type="xs:integer"/>
					<xs:element name="WakeAfterSleepOnset" type="xs:integer"/>
					<xs:element name="LatencyN1" type="xs:integer"/>
					<xs:element name="LatencyN2" type="xs:integer"/>
					<xs:element name="LatencyN3" type="xs:integer"/>
					<xs:element name="LatencyR" type="xs:integer"/>
					<xs:element name="PercenyN1" type="xs:float"/>
					<xs:element name="PercentN2" type="xs:float"/>
					<xs:element name="PercentN3" type="xs:float"/>
					<xs:element name="PercentR" type="xs:float"/>
					<xs:element name="NumberAwakening" type="xs:integer"/>
					<xs:element name="AwakeningIndex" type="xs:float"/>
					<xs:element name="NumberArousals" type="xs:integer"/>
					<xs:element name="ArousalIndex" type="xs:float"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="stmPsgPlm">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="NumberPLM" type="xs:integer"/>
					<xs:element name="NumberPLMS" type="xs:integer"/>
					<xs:element name="NumberPLMW" type="xs:integer"/>
					<xs:element name="NumberPLMS_Arousal" type="xs:integer"/>
					<xs:element name="NumberPLMS_Awakening" type="xs:integer"/>
					<xs:element name="PLMIndex" type="xs:float"/>
					<xs:element name="PLMSIndex" type="xs:float"/>
					<xs:element name="PLMSArousalIndex" type="xs:float"/>
					<xs:element name="PLMSAwakeningIndex" type="xs:float"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="stmPsgBreathing">
		<xs:complexContent>
			<xs:extension base="xnat:subjectAssessorData">
				<xs:sequence>
					<xs:element name="NumberObstructiveApneas" type="xs:integer"/>
					<xs:element name="NumberMixedApneas" type="xs:integer"/>
					<xs:element name="NumberCentralApneas" type="xs:integer"/>
					<xs:element name="NumberHypopneas" type="xs:float"/>
					<xs:element name="NumberApneasHypopneas" type="xs:integer"/>
					<xs:element name="ApneaIndex" type="xs:float"/>
					<xs:element name="HypopneaIndex" type="xs:float"/>
					<xs:element name="ApneaHypopneaIndex" type="xs:float"/>
					<xs:element name="MeanOxygenSaturation" type="xs:float"/>
					<xs:element name="MinimalOxygenSaturation" type="xs:float"/>
					<xs:element name="CheyneStokesBreathing" type="xs:float"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
